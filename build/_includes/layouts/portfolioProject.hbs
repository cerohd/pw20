<!DOCTYPE html>
<html lang="en">
  {{> boilerplate/head }}
  <body>
    {{> boilerplate/siteHeader }}
    
    <div class="post">
      
      {{> projectHeader }}
      
      <ul class="links">
        <li>Project URL: <a href="{{project_url}}" target="_blank">{{project_url}}</a></li>
        <li>Source URL: <a href="{{source_url}}" target="_blank">{{source_url}}</a></li>
      </ul>
      
      {{{ content }}}
      
      <ul>
      {{#each images}}
        <li>{{this}}</li>
      {{/each}}
      </ul>
      
      <hr>
      
      <ul>
        {{#each}}        
          <li>{{@key}}</li>
          <li>{{@value}}</li>
          <li></li>
        {{/each}}
      </ul>
      
    </div>
    
  </body>
</html>