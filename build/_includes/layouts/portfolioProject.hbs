<!DOCTYPE html>
<html lang="en">
  {{> boilerplate/head }}
  <body>
    {{> boilerplate/siteHeader }}
    
    <h2>
      Portfolio
    </h2>
    
    <div class="post">
      
      {{> portfolio/projectHeader }}
      
      <ul class="links">
        <li>Project URL: <a href="{{project_url}}" target="_blank">{{project_url}}</a></li>
        <li>Source URL: <a href="{{source_url}}" target="_blank">{{source_url}}</a></li>
      </ul>
      
      {{{ content }}}
      
      <ul class="project-images">
      {{#each images}}
        <li><img src="{{this}}" /></li>
      {{/each}}
      </ul>
      
    </div>
    
  </body>
</html>