<!DOCTYPE html>
<html lang="en">
  {{> boilerplate/head }}
  <body>
    <head>
      {{> boilerplate/siteHeader }}
    </head>
    <h2>
      Portfolio
    </h2>
    
    <div class="post">
      
      {{> portfolio/projectHeader }}
      
      <ul class="links">
        {{#if project_url}}
          <li>Project URL: <a href="{{project_url}}" target="_blank">{{project_url}}</a></li>
        {{/if}}
        {{#if source_url}}
          <li>Source URL: <a href="{{source_url}}" target="_blank">{{source_url}}</a></li>
        {{/if}}
      </ul>
      
      {{{ content }}}
      
      <ul class="project-images">
      {{#each images}}
        <li><img src="{{this}}" /></li>
      {{/each}}
      </ul>
      
    </div>
    {{> boilerplate/analytics }}
  </body>
</html>