<ol class="postlist">
{{#each collections.postsReversed }}
  <li class="post">
      
    <h3>
      <a href="{{ this.url }}">
        {{#if this.data.title }}
          {{ this.data.title }}
        {{else }}
          <code>
            {{ this.url }}
          </code>
        {{/if}}
      </a>
    </h3>
    
    {{#if this.data.cover_image_url }}
    <img src="{{ this.data.cover_image_url }}" alt="{{ this.data.cover_image_alt }}" />
    {{/if }}
    
    <p>
      {{ this.data.date }}
    </p>
    
    <p>
      {{ this.data.preview }}
    </p>
    
    {{#each this.data }}
    
      <hr /><hr />
        {{#each this }}
      
    
           {{#each this }}
    
    
            {{#each this }}




              {{@../../key}}/{{@../key}}/{{@key}}<br/>
              {{ this }}
              <p>
                +++++++++++ 
              </p>
            {{/each }}
          
    

            {{@key}}<br/>
            <p>
              ////////// 
            </p>
          {{/each }}
          
    
    
          {{@key}}<br/>
          <p>
            ******  
          </p>
        {{/each }}
      <hr /><hr />
    
      {{@key}}!<br/>
      {{ this }}
      <hr />
    {{/each }}
      
      
      
  </li>
{{/each}}
</ol>
