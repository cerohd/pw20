<ol class="postlist">
{{#each collections.postsReversed }}
  <li class="post">
      
    <h3>
      <a href="{{ this.url }}">
        {{#if this.data.title }}
          {{ this.data.title }}
        {{else }}
          <code>
            {{ this.url }}
          </code>
        {{/if}}
      </a>
    </h3>
    
    {{#if cover_image_url }}
    <img src="{{ cover_image_url }}" alt="{{ cover_image_alt_text }}" />
    {{/if }}
    
    <p>
      {{ this.date }}
    </p>
    
    <p>
      
    </p>
      
      
      
  </li>
{{/each}}
</ol>
