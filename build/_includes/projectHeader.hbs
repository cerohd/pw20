<div class="project-header">
  <h3 class="project-name">
    <a href="{{ this.url }}">
      {{#if this.data.name }}
        {{ this.data.name }}
      {{else }}
        <code>
          {{ this.url }}
        </code>
      {{/if}}
    </a>
  </h3>
  
  <div class="tag-container status-tag">
    <div class="tag" style="background-color: #FF6186;">
      complete
    </div>
  </div>

  <p class="project-date-range">{{ projectDate this.data.start_date this.data.end_date }}</p>

</div>