<ol class="postlist">
{{#each collections.projects }}
  <li class="project">
    
    <div class="project-header">
      
      <h3 class="project-name">
        <a href="{{ this.url }}">
          {{#if this.data.title }}
            {{ this.data.title }}
          {{else }}
            <code>
              {{ this.url }}
            </code>
          {{/if}}
        </a>
      </h3>
        
    </div>
    
    
  </li>
{{/each}}
</ol>