<ol class="postlist">
{{#each collections.projects }}
  <li class="project">
    
    <div class="project-header">
      
      <h3 class="project-name">
        <a href="{{ this.url }}">
          {{#if this.data.name }}
            {{ this.data.name }}
          {{else }}
            <code>
              {{ this.url }}
            </code>
          {{/if}}
        </a>
      </h3>
      
      <p class="project-date-range">{{ this.data.start_date}}{{#if this.data.end_date }} - {{ this.data.end_date}}{{/if }}</p>
        
    </div>
    
    
  </li>
{{/each}}
</ol>