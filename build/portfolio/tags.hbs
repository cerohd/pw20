---
pagination:
  data: collections
  size: 1
  alias: tag
  filter:
    - all
    - nav
    - post
    - posts
    - tagList
  addAllPagesToCollections: true
layout: layouts/main.hbs
renderData:
  title: Tagged “{{ tag }}”
permalink: /portfolio/tags/{{ tag }}/
---
<div class="blob">
  <h3>Projects tagged <span class="tag blob-tag">{{ tag }}</span></h3>
</div>

{{> portfolio/index tag=tag }}

<p>See <a href="/tags/">all tags</a>.</p>